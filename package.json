{
  "name": "@aragon/osx-cli",
  "version": "1.0.0",
  "description": "Create, Deploy and Manage Aragon OSx plugins with ease",
  "type": "module",
  "author": "Aragon Association",
  "license": "AGPL-3.0",
  "engines": {
    "node": ">= 16 <19"
  },
  "bin": {
    "aragon": "dist/main.js"
  },
  "scripts": {
    "dev": "node dist/main.js",
    "clean": "rimraf coverage dist tmp",
    "prebuild": "yarn run lint",
    "build": "yarn prebuild && vite build && ./postbuild.sh",
    "build:watch": "vite build -w",
    "build:release": "yarn clean && yarn lint && yarn prettier && yarn build",
    "lint": "eslint . --ext .ts --ext .mts",
    "test": "vitest --watch false",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch --coverage",
    "prettier": "prettier --config .prettierrc --write .",
    "prepublishOnly": "yarn run build"
  },
  "dependencies": {
    "@aragon/osx-ethers": "^1.3.0-rc0",
    "axios": "^1.4.0",
    "chalk": "^5.2.0",
    "chalk-animation": "^2.0.3",
    "commander": "^10.0.1",
    "ethers": "^5.7.2",
    "figlet": "^1.6.0",
    "inquirer": "^9.2.7",
    "inquirer-fuzzy-path": "^2.3.0",
    "install": "^0.13.0",
    "keytar": "^7.9.0",
    "npm": "^9.7.1",
    "ora": "^6.3.1",
    "tslib": "~2.5",
    "web3.storage": "^4.5.4",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@jest/transform": "^29.5.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.0.3",
    "@semantic-release/npm": "^10.0.4",
    "@types/inquirer": "^9.0.3",
    "@types/inquirer-fuzzy-path": "^2.3.6",
    "@types/node": "^20.3.1",
    "@types/qrcode-terminal": "^0.12.0",
    "@typescript-eslint/eslint-plugin": "~5.59",
    "@typescript-eslint/parser": "~5.59",
    "@vitest/coverage-v8": "^0.32.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "~8.8",
    "eslint-plugin-jest": "~27.2",
    "eslint-plugin-vitest": "^0.2.6",
    "prettier": "~2.8",
    "rimraf": "~5.0",
    "semantic-release": "^21.0.7",
    "ts-api-utils": "~0.0.44",
    "ts-node": "^10.9.1",
    "typescript": "~5.0.2",
    "vite": "^4.3.9",
    "vite-plugin-node": "^3.0.2",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.32.0"
  },
  "volta": {
    "node": "18.17.0"
  },
  "packageManager": "yarn@2.4.3"
}
